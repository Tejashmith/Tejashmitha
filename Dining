#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    string s;
    cin>>s;
    int rail;
    cin>>rail;
    if(s=="front" && rail==1)
    {
        cout<<"Left Handed";
    }
    else if(s=="rear" && rail==1)
    {
        cout<<"Right Handed";
    }
    else if(s=="front" && rail==2)
    {
        cout<<"Right Handed";
    }
    else if(s=="rear" && rail==2)
    {
        cout<<"Left Handed";
    }
    return 0;
    
}
