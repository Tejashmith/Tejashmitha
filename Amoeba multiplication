#include<iostream>
using namespace std;
int main()
{
    int month,n1=0,n2=1,n3=0,i;
    cout<<"Enter the number of Months :";
    cin>>month;
    if(month==1)
    {
        n3=0;
    }
    else
    {
    for(i=2;i<month;i++)
    {
        n3=n1+n2;
        n1=n2;
        n2=n3;
    }

    cout<<"The amoeba size is "<<n3;
    }
    return 0;
}
